# VARCOSV configuration file

DEVICE = /dev/ttyUSB0
BAUD_RATE = 57600
PORT = 5000

MY_ID = 69
DEFAULT_DEST = 1

HAS_READER = 0
READER_PORT = /dev/ttyUSB1
READER_BAUD_RATE = 1200

USE_FIFO = N
FIFO_FILE = /tmp/fifotest

USE_TELNET = Y
TELNET_PORT = 6000
TELNET_LATENCY = 10

DB_RAM_FOLDER = /tmp/
DB_PERSISTENT_FOLDER = ./database/
DB_SAVE_TIME = 3600
MAX_EVENTS = 5000

# latency [ms]
LOOP_LATENCY = 100

# dump data from master
DUMP_FROM_MASTER = 0

CHANGE_POLLING = Y
POLLING_TIME = 500
ALIVE_TIME = 5

# UDP debugger
USE_UDP_DEBUG = Y
UDP_DEBUG_IP = 192.168.10.134
UDP_DEBUG_PORT = 5001

ENABLE_CRYPTO = Y
CRYPTO_KEY = default
#CRYPTO_KEY = FA45AD33
